<!DOCTYPE html>
<html>
  <head>
    <meta charset="utf-8">
    <title>{{title}}</title>
  </head>
  <body>
    {{#if engine}}
      <h1>{{engine}}</h1>
    {{else}}
      <h1>No engine found</h1>
    {{/if}}

    <h1>Favorite Foods</h1>
    <ol>
      {{#each foods}}
        <li>{{this}}</li>
      {{/each}}
    </ol>
  </body>
</html>
